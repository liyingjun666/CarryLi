[
  {
    "description": "模拟无参get请求",
    "request": {
      "uri": "/getdemo",
      "method": "get"
    },
    "response": {
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      },
      "text": "这是一个无参请求"
    }
  },
  {
    "description": "模拟有参请求",
    "request": {
      "uri": "/getwithparam",
      "method": "get",
      "queries":{
        "name": "liyingjun",
        "age": "55"
      }
    },
    "response": {
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      },
      "text": "吹梦到西洲"
    }
  }
]